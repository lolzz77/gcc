    "version": "0.2.0",
    "configurations": [
        {
            "type": "cppdbg",
            "request": "launch",
            "name": "GCC DEBUG",
            "program": "/workspace/gcc/objdir/gcc/cc1",
            "cwd" : "/workspace/gcc/",
            "args": [
                "${input:pickProgram}"
            ]
        },
    ],
    "inputs": [
        {
          "id": "pickProgram",
          "description": "Hmm",
          "type": "pickString",
          "options": ["/workspace/test.c"],
          "default": "/workspace/test.c"
        }
    ]
}